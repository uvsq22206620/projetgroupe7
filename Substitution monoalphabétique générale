import random 

mot_à_coder = input("Entrez le mot à coder")
liste = [] 
for e in mot_à_coder :
    liste.append(e)
 
liste2 = liste.copy()

for i in range (len(liste)) :
    liste2[i] = random.randint(0,9)
    for j in range (len(liste)): 
        if liste[i] == liste[j] :
            liste2[j] = liste2[i]
        else :
            while liste2[j] == liste2[i] :
                liste2[j] = random.randint(0,9)
    i += 1


print(liste2)
